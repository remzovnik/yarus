    "@vuelidate/validators": "2.0.0-alpha.29",
    "@vuepic/vue-datepicker": "3.4.7",
    "@vueuse/core": "9.3.0",
    "@vueuse/head": "0.7.6",
    "@vueuse/integrations": "9.3.0",
    "axios": "0.27.2",
    "class-transformer": "0.5.1",
    "floating-vue": "2.0.0-beta.19",
    "hls.js": "1.1.5",
    "imask": "6.4.2",
    "jwt-decode": "3.1.2",
    "mitt": "3.0.0",
    "pinia": "2.0.14",
    "query-string": "7.1.1",
