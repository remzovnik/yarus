{
  "name": "yarus_web",
  "version": "0.0.2",
  "scripts": {
    "serve": "vite preview",
    "build": "vue-tsc --noEmit && vite build",
    "build:dev": "vue-tsc --noEmit && vite build --mode development",
    "build:prod": "vue-tsc --noEmit && vite build --mode production",
    "generate": "vue-tsc --noEmit && vite build",
    "dev": "vue-tsc --noEmit && vite --mode development",
    "format": "prettier .  --write --fix",
    "lint:script": "eslint --ext .ts,vue --ignore-path .gitignore --fix src",
    "lint:style": "stylelint src/**/*.{css,scss,vue} --fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "cd .. && husky install app/.husky"
  },
  "dependencies": {
    "@types/grecaptcha": "3.0.4",
    "@types/json-bigint": "^1.0.1",
    "@types/youtube": "^0.0.47",
