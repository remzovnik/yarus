    "vue-advanced-cropper": "2.8.1",
    "vue-fullscreen": "^3.1.1",
    "vue-multiselect": "3.0.0-alpha.2",
    "vue-router": "4.0.15",
    "vue3-lottie": "^2.5.0",
    "vue3-runtime-template": "^1.0.2",
    "vuedraggable": "4.1.0",
    "webfontloader": "1.6.28",
    "yandex-metrika-vue3": "^1.1.6",
    "ymaps": "0.2.1"
